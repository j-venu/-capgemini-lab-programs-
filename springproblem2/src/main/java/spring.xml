<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<bean id="emp" class="com.capg.springproblem2.Employee" autowire="byType">
	<property name="employeeId" value="18"></property>
	<property name="employeeName" value="venuvirat"></property>
	<property name="salary" value="30000"></property>
	<property name="age" value="22"></property>
	</bean>
	<bean id="sbu" class="com.capg.springproblem2.SBU">
	<property name="sbuId" value="100"></property>
	<property name="sbuName" value="PES-BU"></property>
	<property name="sbuHead" value="venu"></property>
	</bean>

</beans>
